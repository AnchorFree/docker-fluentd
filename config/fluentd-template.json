{
    "template" : "fluentd-*",
    "settings" : {
      "number_of_shards" : 22,
      "index" : {
        "refresh_interval" : "30s"
      }
    },
    "mappings": {
    "_default_": {
      "_all": {
        "enabled": true
      },
      "dynamic_templates": [
        {
          "string_fields": {
            "mapping": {
              "index": "analyzed",
              "omit_norms": true,
              "type": "string",
              "fields": {
                "raw": {
                  "ignore_above": 256,
                  "index": "not_analyzed",
                  "type": "string"
                }
              }
            },
            "match_mapping_type": "string",
            "match": "*"
          }
        }
      ],
      "properties": {
        "beat": {
          "type": "object",
          "properties": {
            "hostname": {
              "type": "string",
              "index": "not_analyzed"
            },
            "version": {
              "type": "string",
              "index": "not_analyzed"
            },
            "name": {
              "type": "string",
              "index": "not_analyzed"
            }
          }
        },
        "rsync_distribution": {
          "type": "string",
          "index": "not_analyzed"
        },
